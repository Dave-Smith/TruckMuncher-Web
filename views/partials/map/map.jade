#wrapper
    #sidebar-wrapper.bg-color-mold
        div(data-ng-include="'/partials/map/onlineList.jade'", style="padding-top:70px;")


    #page-content-wrapper.tm-main
        #content
            .col-lg-10-col-lg-offset-1.col-md-10.col-md-offset-1.col-xs-12.map-padding
                article(data-ng-style="{'height': '{{mapHeight}}'}")
                    ui-gmap-google-map(center="map.center", data-ng-if='mapHeight' data-refresh='true', zoom="map.zoom", draggable="true", options="map.options", data-ng-style="{'height': '{{mapHeight}}'}")
                        ui-gmap-markers(models='displayedMarkers', idKey='"id"', doCluster='true', coords='"coords"', show='true', click='onMarkerClicked' options='"options"')
                        ui-gmap-window(coords='infoWindow.coords', show='infoWindow.show', templateUrl='infoWindow.templateUrl', templateParameter='infoWindow.templateParameter', closeClick='closeInfoWindow', options='infoWindow.options')
                        ui-gmap-marker(data-ng-if='currentPositionMarker.id', idKey='currentPositionMarker.id', coords='currentPositionMarker.coords', icon='currentPositionMarker.icon')
                            ui-gmap-window(coords='locationWindow.coords', show='locationWindow.show')
                                div Your Position

        .visible-xs.col-xs-12(style="padding:25px")
            div(data-ng-include="'/partials/map/onlineList.jade'")

