//nav#nav-top.navbar.navbar-default(data-ng-cloak='ng-cloak', data-ng-controller='navCtrl')
#nav-top(data-ng-controller='navCtrl')
    .container-fluid(style="float:right")
        div
            span.btn-header(data-ng-show='loggedIn()')
                span
                    a(href="/logout", title='Sign Out')
                        i.fa.fa-sign-out
            span.btn-header(data-ng-hide='loggedIn()')
                span
                    a(href="/logout", title='Sign In')
                        i.fa.fa-sign-in
